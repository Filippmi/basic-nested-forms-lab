<%= form_for @recipe do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %> <br>

  <%= f.fields_for :ingredients do |ib| %>
    <%= ib.label :name %>
    <%= ib.text_field :name %><br/>
    
    <%= ib.label :quantity %>
    <%= ib.text_field :quantity %><br/>
  <% end %>
  <%= f.submit %>
<% end %>
